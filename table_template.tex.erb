\begin{table}[h]
    \centering
    <%- if !caption.empty? then -%>
    \caption{<%= caption %>}
    \label{tab:<%= caption %>}
    <%- end -%>
    \begin{tabular}{<%= dimensions %>} \hline
      <%- table.each do |row| -%>
        <%= row.join(" & ") + " \\\\ \\hline" %>
      <%- end -%>
    \end{tabular}
\end{table}
