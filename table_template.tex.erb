<%- if option[:docmute] then -%>
\documentclass[a4paper]{jsarticle}

\usepackage{docmute}

\begin{document}

<%- end -%>
\begin{table}[h]
    \centering
    <%- if !caption.empty? then -%>
    \caption{<%= caption %>}
    \label{tab:<%= caption %>}
    <%- end -%>
    \begin{tabular}{<%= dimensions %>} \hline
      <%- table.each do |row| -%>
        <%= row.join(" & ") + " \\\\ \\hline" %>
      <%- end -%>
    \end{tabular}
\end{table}
<%- if option[:docmute] then -%>

\end{document}
<%- end -%>
