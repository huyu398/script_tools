<%- if option[:docmute] then -%>
\documentclass[a4paper]{jsarticle}

\usepackage{docmute}

\begin{document}

<%- end -%>
\begin{table}[h]
    \centering
    <%- if !caption.empty? then -%>
    \caption{<%= caption %>}
    \label{tab:<%= caption %>}
    <%- end -%>
    \begin{tabular}{<%= dimensions %>}
      <%- if option[:frame] and option[:frame][:top] then -%>
        \hline
      <%- end -%>
        <%= table.shift.join(' & ') -%>
      <%- table.each do |row| -%>
        <%- %><%= " \\\\ \\hline" %>
        <%= row.join(" & ") -%>
      <%- end -%>
      <%- if option[:frame] and option[:frame][:bottom] then -%>
        <%- %> \\ \hline
      <%- else %>
      <%- end -%>
    \end{tabular}
\end{table}
<%- if option[:docmute] then -%>

\end{document}
<%- end -%>
